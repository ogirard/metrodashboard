<ResourceDictionary x:Class="ZTG.WPF.Dashboard.Shared.UserInterface.VisualResources.Resources"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <ResourceDictionary.MergedDictionaries />

    <!--
        The following Style is needed to fix an issue with merging dictionaries inside an empty dictionary in .NET 4.0
        See also http://www.wpftutorial.net/MergedDictionaryPerformance.html
    -->
    <Style TargetType="{x:Type Ellipse}" />
</ResourceDictionary>